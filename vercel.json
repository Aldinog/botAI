{
  "version": 2,
  "routes": [
    {
      "src": "/webhook",
      "dest": "api/webhook.js",
      "methods": [
        "POST"
      ]
    },
    {
      "src": "/api/web",
      "dest": "api/web.js",
      "methods": [
        "POST",
        "OPTIONS"
      ]
    },
    {
      "src": "/api/cron/screener",
      "dest": "api/cron/screener.js",
      "methods": [
        "POST",
        "GET"
      ]
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}